# SmartImport 4.0 Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_BASE_URL=https://api.exceltta.com
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_OCR=true
VITE_ENABLE_AI_CLASSIFICATION=true
VITE_ENABLE_REAL_TIME_UPDATES=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# External Services
VITE_EXCHANGE_RATE_API_KEY=
VITE_NCM_API_URL=https://api.ncm.exceltta.com

# PWA Configuration
VITE_PWA_NAME=SmartImport 4.0
VITE_PWA_SHORT_NAME=SmartImport
VITE_PWA_DESCRIPTION=Simulador Estratégico de Operações de Importação com IA

# Development
VITE_DEV_MODE=false
VITE_ENABLE_DEBUG_LOGS=false 

CERT_PASSWORD=123456

OPENAI_API_KEY=your_openai_api_key_here
